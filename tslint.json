{
    "extends": "tslint:latest",
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "member-ordering": [
            false
        ],
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false
    }
}